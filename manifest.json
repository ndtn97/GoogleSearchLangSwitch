{
    "name": "Google Lang Switch",
    "description": "",
    "version": "0.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "/js/background.js"
    },
    "permissions": [
        "declarativeNetRequest",
        "tabs"
    ],
    "host_permissions": [
        "*://www.google.com/search*"
    ],
    "action": {},
    "icons": {
        "128": "/images/icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://www.google.com/search*"
            ],
            "js": [
                "/js/page.js"
            ],
            "css": [
                "/css/clean-switch.css"
            ],
            "run_at": "document_end"
        }
    ]
}
